{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "allowJs": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@actions": ["./src/actions"],
      "@actions/*": ["./src/actions/*"],
      "@api": ["./src/api"],
      "@api/*": ["./src/api/*"],
      "@assets": ["./src/assets"],
      "@assets/*": ["./src/assets/*"],
      "@auth": ["./src/auth"],
      "@auth/*": ["./src/auth/*"],
      "@components": ["./src/components"],
      "@components/*": ["./src/components/*"],
      "@config": ["./src/config"],
      "@config/*": ["./src/config/*"],
      "@contexts": ["./src/contexts"],
      "@contexts/*": ["./src/contexts/*"],
      "@dds": ["./src/dds"],
      "@dds/*": ["./src/dds/*"],
      "@enums": ["./src/enums"],
      "@enums/*": ["./src/enums/*"],
      "@errors": ["./src/errors"],
      "@errors/*": ["./src/errors/*"],
      "@fragments": ["./src/fragments"],
      "@fragments/*": ["./src/fragments/*"],
      "@hooks": ["./src/hooks"],
      "@hooks/*": ["./src/hooks/*"],
      "@mock": ["./src/mock"],
      "@mock/*": ["./src/mock/*"],
      "@normalizr": ["./src/normalizr"],
      "@normalizr/*": ["./src/normalizr/*"],
      "@reducers": ["./src/reducers"],
      "@reducers/*": ["./src/reducers/*"],
      "@screens": ["./src/screens"],
      "@screens/*": ["./src/screens/*"],
      "@selectors": ["./src/selectors"],
      "@selectors/*": ["./src/selectors/*"],
      "@services": ["./src/services"],
      "@services/*": ["./src/services/*"],
      "@socket": ["./src/socket"],
      "@socket/*": ["./src/socket/*"],
      "@store": ["./src/store"],
      "@store/*": ["./src/store/*"],
      "@styles": ["./src/styles"],
      "@styles/*": ["./src/styles/*"],
      "@types": ["./src/types"],
      "@types/*": ["./src/types/*"],
      "@utils": ["./src/utils"],
      "@utils/*": ["./src/utils/*"],
      "@vendor": ["./vendor"],
      "@vendor/*": ["./vendor/*"],
      "@": ["./src"],
      "@/*": ["./src/*"]
    }
  },
  "include": ["src"]
}
